<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activities.NewXmlActivity">

    <EditText
        android:id="@+id/numberText"
        android:layout_width="381dp"
        android:layout_height="43dp"
        android:layout_marginTop="56dp"
        android:ems="10"
        android:hint="@string/enter_the_set_number_f_e_615"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.533"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="TextFields" />

    <EditText
        android:id="@+id/projectNameText"
        android:layout_width="381dp"
        android:layout_height="43dp"
        android:ems="10"
        android:enabled="false"
        android:hint="@string/enter_project_name"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.533"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/numberText"
        android:autofillHints="" />

    <Button
        android:id="@+id/checkButton"
        android:layout_width="170dp"
        android:layout_height="53dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:onClick="load"
        android:text="@string/check"
        app:layout_constraintBottom_toTopOf="@+id/textView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/projectNameText"
        app:layout_constraintVertical_bias="0.503" />

    <Button
        android:id="@+id/addButton"
        android:layout_width="170dp"
        android:layout_height="53dp"
        android:enabled="false"
        android:onClick="add"
        android:text="@string/add"
        app:layout_constraintBottom_toTopOf="@+id/textView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.709"
        app:layout_constraintStart_toEndOf="@+id/checkButton"
        app:layout_constraintTop_toBottomOf="@+id/projectNameText"
        app:layout_constraintVertical_bias="0.503" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="297dp"
        android:layout_height="386dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>